.bottom-menu{
    width: 100%;
    height: 3rem;
    background: #FFF;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 90;
    border-top:1px solid #CBCBCB;
}  
.bottom-menu div{
    text-align: center;
    line-height: 3rem;
    border-right: 1px solid #CBCBCB;
}
.bottom-menu div:last-child{
    border-right:none;
}
.bottom-menu .index-menu{
    height: 3rem;
    background: url("../image/home.svg") no-repeat center;
    background-size: 20px;
}
.bottom-menu .sign-menu{
    background: url("../image/menu.svg") no-repeat 10px 0.9rem;
    background-size: 16px;
}
.bottom-menu .menu-list a{color: #4A4A4A;}
.sign-menu{
            position: relative;
        }
        .menu-list #sign-menu,.menu-list #sign-menu div{border-right:none;}
        .sing-menu-list{
            width:6rem;
            height: 6rem;
            padding: 0 1rem;
            background: #FFF;
            border-radius: 0.2rem;
            position: absolute;
            left: 0.5rem;
            top:-6.4rem;
            border-right: none;
            display: none;
        }
        .sing-menu-list:after{
            content: "";
            width: 0;
            height: 0;
            border-left: 0.4rem solid transparent;
            border-right: 0.4rem solid transparent;
            border-top: 0.8rem solid #FFF;
            position: absolute;
            bottom: -0.5rem;
            left: 2.6rem;
        }
        .sing-menu-list p{
            font-size: 12px;
            border-bottom: 1px solid #C9C9C9;
        }
        .sing-menu-list p:last-child{border-bottom: none;}

