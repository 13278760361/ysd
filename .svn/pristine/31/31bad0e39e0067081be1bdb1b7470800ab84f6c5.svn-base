   .sing-user{
            height: 6rem;
            padding: 1rem;
            margin: 1.5rem 0.5rem;
            background: #FFF;
            border-radius: 0.2rem;
            box-shadow: 0px 1px 2px 1px #B2B2B2;
        }
        .sing-user-img{
            height: 4rem;
            width: 4rem;
            border-radius: 100rem;
            overflow: hidden;
        }
        .sing-user-img img{width: 4rem;height: 4rem;}
        .sing-user-info{padding-left: 1.2rem;}
        .sing-user-info div{line-height: 1.5; color: #222222;}
        
        .course-list{
            height: 4rem;
            margin: 1.5rem 0.5rem;
            background: #FFF;
            padding: 0.5rem 1rem;
            border-radius: 0.2rem;
            box-shadow: 0px 1px 2px 1px #B2B2B2;
        }
        .course-name{height: 1.5rem;font-size: 14px; }
        .rang{height: 0.2rem;background: #DEE9DF;}
        .speed{width: 50%; height: 0.2rem;background: #09BB07;}
        .rang-tips{
            height: 1.3rem;
            position: relative;
            backgrounds: red;
        }
        .tips-box{
            width: 3rem;
            height: 1rem;
            position: absolute;
            bottom: 0;
            border-radius: 0.1rem;
        }
        .tips-box-c1{background: #09BB07;}
        .tips-box-c2{background: #D0D0D0;}
        .tips-box .arrow{
            position: absolute;
            top:-0.3rem;
            width: 0px;
            height: 0px;
            /*以下属性可以是IE5兼容*/
            font-size: 0px;
            line-height: 0px;
        }
        .tips-box p{
            font-size: 0.7rem;
            text-align: center;
            line-height: 1.1rem;
            color: #FFF;
        }
        .arrow-left{
            left: 0;
            border-left: 0 solid transparent;
            border-right: 0.4rem solid transparent;
            border-bottom: 0.4rem solid #D0D0D0;
        }
        .arrow-center{
            left: 0.7rem;
            border-left: 0.4rem solid transparent;
            border-right: 0.4rem solid transparent;
            border-bottom: 0.4rem solid #09BB07;
        }
        .arrow-right{
            right: 0;
            border-left: 0.4rem solid transparent;
            border-right: 0rem solid transparent;
            border-bottom: 0.4rem solid #09BB07;
        }
        .bottom-menu{
    width: 100%;
    height: 3rem;
    background: #FFF;
    position: fixed;
    left: 0;
    bottom: 0;
    border-top:1px solid #CBCBCB;
}  
.bottom-menu div{
    text-align: center;
    line-height: 3rem;
    border-right: 1px solid #CBCBCB;
}
.bottom-menu div:last-child{
    border-right:none;
}
.bottom-menu .index-menu{
    height: 3rem;
    background: url("../image/home.svg") no-repeat center;
    background-size: 20px;
}
.bottom-menu .sign-menu{
    background: url("../image/menu.svg") no-repeat 10px 0.9rem;
    background-size: 16px;
}
.bottom-menu .menu-list a{color: #4A4A4A;}
.sign-menu{
            position: relative;
        }
        .menu-list #sign-menu,.menu-list #sign-menu div{border-right:none;}
        .sing-menu-list{
            width:6rem;
            height: 6rem;
            padding: 0 1rem;
            background: #FFF;
            border-radius: 0.2rem;
            position: absolute;
            left: 0.5rem;
            top:-6.4rem;
            border-right: none;
            display: none;
        }
        .sing-menu-list:after{
            content: "";
            width: 0;
            height: 0;
            border-left: 0.4rem solid transparent;
            border-right: 0.4rem solid transparent;
            border-top: 0.8rem solid #FFF;
            position: absolute;
            bottom: -0.5rem;
            left: 2.6rem;
        }
        .sing-menu-list p{
            font-size: 12px;
            border-bottom: 1px solid #C9C9C9;
        }
        .sing-menu-list p:last-child{border-bottom: none;}